<SCRIPT LANGUAGE="JavaScript" TYPE="text/javascript">
//<!--
var periodsSelected = 2 ;
var comparisonType = "months" ;

if (parseInt(navigator.appVersion) >= 4) {
	isNav = (navigator.appName == "Netscape")&&(parseInt(navigator.appVersion) < 5);
	isNav06 = (navigator.appName == "Netscape")&&(parseInt(navigator.appVersion) >= 5);
	isIE = (navigator.appName.indexOf("Microsoft Internet Explorer") != -1);
	isMacIE = (navigator.userAgent.indexOf("MSIE") != -1) && (navigator.userAgent.indexOf("Mac") != -1) && (parseInt(navigator.appVersion) >= 4);
}

function showBlocks() {
//	alert('You have selected ' + periodsSelected + ' periods by ' + comparisonType) ;
	for (ii=1;ii<=9;ii++) {
		if (ii > periodsSelected) {
			//hide...
			hide(ii) ;
		} else {
			//show...
			show(ii) ;
		}
	}
}

function hide(id) {
	if (isNav){
		document.layers[id].visibility = 'hide' ;
	} else if (isIE){
		//document.all[id].style.visibility = 'hidden' ;
		eval("document.forms[0].day" + id + ".style.visibility = 'hidden'")
		eval("document.forms[0].month" + id + ".style.visibility = 'hidden'")
		eval("document.forms[0].year" + id + ".style.visibility = 'hidden'")
	} else if (isNav06){    
		document.getElementById(id).style.visibility = 'hidden';
	}
}

function show(id) {
	if (isNav){
		document.layers[id].visibility = 'hide' ;
	} else if (isIE){
		//document.all[id].style.visibility = 'hidden' ;
		if (comparisonType == "days" ) {
			eval("document.forms[0].day" + id + ".style.visibility = 'visible'")
		} else {
			eval("document.forms[0].day" + id + ".style.visibility = 'hidden'")
		}
		if ( (comparisonType == 'days') || (comparisonType == 'months')) {
			eval("document.forms[0].month" + id + ".style.visibility = 'visible'")
		} else {
			eval("document.forms[0].month" + id + ".style.visibility = 'hidden'")
		}
		eval("document.forms[0].year" + id + ".style.visibility = 'visible'")
	} else if (isNav06){    
		document.getElementById(id).style.visibility = 'hidden';
	}
}

//-->
</script>

